{
	"name": "web-experiments-sveltekit-socketio-express",
	"version": "0.0.1",
	"private": true,
	"scripts": {
        "server":"rm -rf dist && node server/esbuild.mjs",
        "server_static":"rm -rf dist && node server_static/esbuild_static.mjs",		
		"build": "MODE=NODE vite build && npm run server",
        "build_static": "MODE=STATIC vite build && npm run server_static",        
        "start":"node build/server.js",
        "start_static":"node dist/server_static.cjs",
        "restart":"npm run build && npm run start",
        "restart_static":"npm run build_static && npm run start_static",
        "dev": "vite dev",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.20.4",
		"@types/express": "^4.17.17",
		"express": "^4.18.2",
		"socket.io": "^4.7.1",
		"socket.io-client": "^4.7.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module"
}
