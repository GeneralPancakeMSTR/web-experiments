{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap":true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "lib": ["es6","dom"],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}